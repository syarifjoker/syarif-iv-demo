{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@mockData": ["./resources/jsonData"],
      "@mockData/*": ["./resources/jsonData/*"],
      "@components/*": ["./src/components/*"],
      "@screens/*": ["./src/screens/*"],
      "@assets": ["./src/assets"],
      "@assets/*": ["./src/assets/*"],
      "@constants": ["./src/constants"],
      "@constants/*": ["./src/constants/*"]
    }
  }
}
